---
title: Default layout
is_layout: false
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{ site.name }}</title>
    <link rel="shortcut icon" href="{{ 'favicon.png' | theme_image_url }}">
    <link id="bs" rel="stylesheet" type="text/css" href="">
    {{ 'application.css' | stylesheet_tag }}
  </head>
  <body style="padding-top: 70px;" data-spy="scroll" data-target="#navbar">
    {% include 'nav' %}
    {% block content %}
    {% endblock %}
    {% include 'footer' %}
    {{ 'jquery.min.js' | javascript_tag }}
    {{ 'bootstrap.min.js' | javascript_tag }}
    {{ 'application.js' | javascript_tag }}
  </body>
</html>
